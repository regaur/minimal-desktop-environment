		<Include>
			<Filename>panel-desktop-handler.desktop</Filename>
		</Include>
		<Layout>
			<Merge type="menus" />
			<Filename>bvnc.desktop</Filename>
			<Filename>bssh.desktop</Filename>
			<Filename>compiz.desktop</Filename>
			<Filename>panel-desktop-handler.desktop</Filename>
			<Merge type="files" />
		</Layout>
	</Menu>
